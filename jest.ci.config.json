{
    "setupFilesAfterEnv": [
        "./tests/magic-odata-tests/dist/tests/magic-odata-tests/setup-jest.js"
    ],
    "roots": [
        "./"
    ],
    "testMatch": [
        "**/tests/magic-odata-tests/**/*.spec.js"
    ],
    "coveragePathIgnorePatterns": [
        "argParser\\.js$",
        "codeGenApp\\.js$",
        "codeGenCmd\\.js$",
