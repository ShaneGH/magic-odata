{
  "name": "magic-odata-tests-browser",
  "version": "0.1.1",
  "scripts": {
    "fix-links: COMMENT": "Used to link the test project to the modules that it is testing. This command needs to be run after each 'npm install'",
    "fix-links": "npm link ../../magic-odata-client ../../magic-odata-code-gen ../magic-odata-tests",
    "start-test-server": "node ./node_modules/magic-odata-client-tests/startTestServer.js",
    "stop-test-server": "node ./node_modules/magic-odata-client-tests/stopTestServer.js",
    "generate-code-fetch": "node node_modules/magic-odata-code-gen/dist/index.js --config ./src/clients/code-gen-config-fetch.json",
    "generate-code-angular": "node node_modules/magic-odata-code-gen/dist/index.js --config ./src/clients/code-gen-config-angular-string.json",
    "generate-code-angular-blob": "node node_modules/magic-odata-code-gen/dist/index.js --config ./src/clients/code-gen-config-angular-blob.json",
    "generate-code-angular-arraybuffer": "node node_modules/magic-odata-code-gen/dist/index.js --config ./src/clients/code-gen-config-angular-arraybuffer.json",
    "start-server-and-test": "(npm run start-test-server && npm run generate-code-fetch && npm run generate-code-angular && npm run generate-code-angular-blob && npm run generate-code-angular-arraybuffer && tsc && npm run test-only)",
    "test": "(npm run start-test-server && npm run generate-code-fetch && npm run generate-code-angular && npm run generate-code-angular-blob && npm run generate-code-angular-arraybuffer && tsc && npm run test-only)",
    "build-and-test-win": "node_modules\\magic-odata-client-tests\\buildFromRoot.cmd && npm run start-server-and-test",
    "build-and-test-unix": "node_modules/magic-odata-client-tests/buildFromRoot.sh && npm run test",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test-only": "ng test --watch false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0",
    "magic-odata-client": "0.1.1",
    "magic-odata-code-gen": "0.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}